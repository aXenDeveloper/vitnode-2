{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "builder": {
      "type": "swc",
      "options": {
        "outDir": "dist/src",
        "extensions": [".ts", ".tsx"]
      }
    },
    "typeCheck": true
  }
}
